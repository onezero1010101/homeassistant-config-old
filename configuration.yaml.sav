homeassistant:
  # Name of the location where Home Assistant is running
  name: Home
  # Location required to calculate the time the sun rises and sets
  latitude: 38.623597
  longitude: -85.724111
  # Impacts weather/sunrise data
  elevation: 175
  # C for Celsius, F for Fahrenheit
  #temperature_unit: F
  unit_system: imperial
  # Pick yours from here: http://en.wikipedia.org/wiki/List_of_tz_database_time_zones
  time_zone: America/Indiana/Indianapolis

  customize:
    sensor.speedtest_ping:
      friendly_name: 'Ping Time'
      icon: mdi:speedometer
    sensor.speedtest_download:
      friendly_name: 'Download'
      icon: mdi:download
    sensor.speedtest_upload:
      friendly_name: 'Upload'
      icon: mdi:upload
    media_player.roku_63259d009925:
      friendly_name: 'Living Room Roku'
    media_player.roku_13c27n041227:
      friendly_name: 'Bedroom Roku'
    sensor.moviebox_transmission_down_speed:
      friendly_name: 'Transmission Download'
      icon: mdi:download
    sensor.moviebox_transmission_up_speed:
      friendly_name: 'Transmission Upload'
      icon: mdi:upload
  
group:
  default_view:
    view: yes
    entities:
      - sun.sun
      - group.stove
      - switch.puff_light
      - switch.rainbow_light
  stats_tab:
    name: Stats
    view: yes
    entities:
      - group.internet
      - group.moviebox
      - group.family
      - group.homeasst
      - switch.test
  media_tab:
    name: Media
    view: yes
    entities:
      - media_player.roku_63259d009925
      - media_player.roku_13c27n041227
      - media_player.roku_3
  util_tab:
    name: Utililitys
    view: yes
    entities:
      - weblink.nodered
      - weblink.sabnzbd
  internet:
    name: Internet Capacity
    entities:
      - sensor.speedtest_ping
      - sensor.speedtest_download
      - sensor.speedtest_upload
      - sensor.fastcom_speedtest
  moviebox:
    name: Moviebox
    entities:
      - sensor.moviebox_transmission_down_speed
      - sensor.moviebox_transmission_up_speed
  family:
    name: Family
    entities:
      - device_tracker.mike_phone
      - sensor.mike_phone_battery
      - device_tracker.tammy_phone
  homeasst:
    name: Home Assistant Status
    entities:
      - sensor.cpu_use
      - sensor.ram_free
      - sensor.disk_use_home
      - sensor.last_boot
      - sensor.since_last_boot
  stove:
    name: Stove
    entities:
      - sensor.stove_humidity
      - sensor.stove_temperature

weblink:
  entities:
    - name: Node-Red
      url: http://192.168.9.42:1880
    - name: Sabnzbd
      url: http://192.168.9.10:8080


