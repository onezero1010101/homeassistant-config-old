automation: 
- alias: Update notifications
  trigger: 
    - platform: state
      entity_id: updater.updater
  action: 
    service: notify.instapush
    data:
      message: 'There is a new Home Assistant release available'
- alias: Send reminder
  trigger:
    platform: time
    hours: 17
    minutes: 3
    seconds: 0
  action:
    service: notify.instapush
    data:
      message: 'Reminder activated'
- alias: Reptile Lights
  trigger:
    platform: time
    hours: 7
    minutes: 30
    seconds: 0
  action:
    service: homeassistant.turn_on
    entity_id:
      - switch.puff_light
      - switch.rainbow_light
- alias: Reptile Off
  trigger:
    platform: time
    hours: 21
    minutes: 30
    seconds: 0
  action:
    service: homeassistant.turn_off
    entity_id:
      - switch.puff_light
      - switch.rainbow_light

- alias: Mike at Work
  trigger: 
    platform: zone
    entity_id: device_tracker.mike_phone
    zone: zone.Work
    event: enter
  action:
    service: notify.instapush 
    data: 
      message: "Mike has arrived at work"
- alias: Mike left Work
  trigger: 
    platform: zone
    entity_id: device_tracker.mike_phone
    zone: zone.Work
    event: leave
  action:
    service: notify.instapush 
    data: 
      message: "Mike has left work"
- alias: Tammy Home
  trigger:
    platform: state
    entity_id: device_tracker.tammy_phone
    to: 'home'
  action: 
    service: notify.instapush
    data: 
      message: "Tammy is home"
- alias: Tammy Not Home
  trigger:
    platform: state
    entity_id: device_tracker.tammy_phone
    to: 'not_home'
  action: 
    service: notify.instapush
    data: 
      message: "Tammy is away from home"

